Options +FollowSymLinks

php_value max_execution_time 300


RewriteEngine On
RewriteRule ^([a-z]+)/([a-z\-]+)?$ in&outadmin.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ testnotes.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ welcomeadmin.php

RewriteRule ^status/([0-1000]+) status.php?id=$engineer_id



RewriteRule ^([a-z]+)/([a-z\-]+)?$ utilization.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ schedule.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ adminschedule.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ adminutilization.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ alldeduction.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ allleaves.php

RewriteRule ^([a-z]+)/([a-z\-]+)?$ allsign.php

RewriteRule ^allsign/(\d+)*$ ./allsign.php?engineer_id=$1
RewriteRule ^allsign/([a-z0-9_\-]+)/$ allsign.php?engineer_id=$0 [L,NC]

RewriteRule ^([a-zA-Z0-9]+)$ allsign.php?engineer_id=$1
RewriteRule ^([a-zA-Z0-9]+)/$ allsign.php?engineer_id=$1


RewriteRule ^note2/([a-z]+)/([A-Z\-]+)?$ testnote.php?engineer_id=$

RewriteRule ^([a-z]+)/([a-z\-]+)?$ allsignView.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ allstatus.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ allutilization.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ deductionadmin.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ deductios.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ deductionstatus.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ deductionupdate.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ deductionview.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ edit_password.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ in&outAdmin.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ leaves.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ leavesapproval.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ leavesstatus.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ leavesview.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ Notes_page.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ signView.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ status.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ taskstatus.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ tasksview.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ tasktest.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ utilizationadmin.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ viewdeduction.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ schedule_update.php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ .php
RewriteRule ^([a-z]+)/([a-z\-]+)?$ .php
<ifModule mod_headers.c>
    Header set Connection keep-alive
    # Enable expirations
    ExpiresActive On 
    # Default directive
    ExpiresDefault "access plus 1 month"
    # My favicon
    ExpiresByType image/x-icon "access plus 1 year"
    # Images
    ExpiresByType image/gif "access plus 1 month"
    ExpiresByType image/png "access plus 1 month"
    ExpiresByType image/jpg "access plus 1 month"
    ExpiresByType image/jpeg "access plus 1 month"
    # CSS
    ExpiresByType text/css "access plus 1 month"
    # Javascript
    ExpiresByType application/javascript "access plus 1 year"
</IfModule>

